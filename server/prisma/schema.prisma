// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Board {
  idx          Int      @id @default(autoincrement())
  board_key    String
  writer_key   String
  writer       String
  title        String
  content_text String?  @db.Text
  is_use       Boolean  @default(true)
  create_date  DateTime @default(now())

  @@map("cms_board_data")
}
